};

&pwm4 {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_pwm4>;
	#pwm-cells = <3>;
	status = "okay";
};

&mipi_csi {
        status = "okay";
        ipu_id = <0>;
        csi_id = <1>;
        v_channel = <0>;
        lanes = <1>;
};

&spdif {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_spdif_1>;
        status = "disabled";
};
